{
  "name": "musichat",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "generate:sqlite": "drizzle-kit generate --config=drizzle.config.sqlite.ts",
    "generate:supabase": "drizzle-kit generate --config=drizzle.config.supabase.ts",
    "migrate:sqlite": "tsx scripts/migrate-sqlite.ts",
    "migrate:supabase": "tsx scripts/migrate-supabase.ts",
    "db:reset:sqlite": "rm -f musichat.db && pnpm generate:sqlite && pnpm migrate:sqlite",
    "db:push:supabase": "drizzle-kit push --config=drizzle.config.supabase.ts",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.4.1",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.90.5",
    "babel-preset-expo": "~54.0.9",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.7",
    "expo": "~54.0.20",
    "expo-background-fetch": "^14.0.9",
    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-task-manager": "^14.0.9",
    "nanoid": "^5.1.6",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-uuid": "^2.0.3",
    "zod": "^4.2.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/pg": "^8.15.5",
    "@types/react": "~19.1.0",
    "babel-plugin-inline-import": "^3.0.0",
    "drizzle-kit": "^0.31.5",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  },
  "private": true
}